alias jupy="jupyter notebook"

alias jlab="jupyter lab"
alias ..="cd .."
alias ...="cd ../.."
alias ....="cd ../../.."
alias ~="cd ~"
alias py3=python3
alias t="tmux"
alias ta="t a -t"
